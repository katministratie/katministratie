@using Superkatten.Katministratie.Application.Contracts

<div class="p-1 m-1 border border-primary">
    <button onclick="@OnClickCallback">@ActionName</button>
</div>

@code {
    [Parameter]
    public string ActionImage { get; set; } = "images/superkat.jpg";

    [Parameter]
    public string ActionName { get; set; } = "unknown";

    [Parameter]
    public EventCallback OnClickCallback { get; set; }
}
